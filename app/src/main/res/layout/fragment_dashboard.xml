<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>


        <variable
            name="listener"
            type="android.view.View.OnClickListener" />

    </data>

    <androidx.core.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/colorWhite"
        tools:context=".ui.component.dashboard.DashboardFragment">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingBottom="@dimen/_7sdp">

            <androidx.cardview.widget.CardView
                android:id="@+id/cvCarePoints"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginHorizontal="@dimen/_10sdp"
                android:layout_marginTop="@dimen/_7sdp"
                android:onClick="@{listener::onClick}"
                app:cardBackgroundColor="@color/_5F6AB2"
                android:visibility="gone"
                app:cardCornerRadius="@dimen/_18ssp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:padding="@dimen/_15sdp"
                    android:background="@drawable/ic_care_points">

<!--
                    <androidx.appcompat.widget.AppCompatImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/ic_care_points"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent" />
-->

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintTop_toTopOf="parent">

                        <androidx.appcompat.widget.AppCompatImageView
                            android:id="@+id/ivCarePoints"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@drawable/ic_home_calender"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />

                        <androidx.appcompat.widget.AppCompatTextView
                            android:id="@+id/tvCarePoint"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_5sdp"
                            android:fontFamily="@font/gotham_medium"
                            android:text="@string/care_points"
                            android:textColor="@color/colorWhite"
                            android:textSize="@dimen/_18ssp"
                            app:layout_constraintStart_toEndOf="@+id/ivCarePoints"
                            app:layout_constraintTop_toTopOf="@+id/ivCarePoints" />

                        <androidx.appcompat.widget.AppCompatTextView
                            android:id="@+id/tvNoEvent"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_2sdp"
                            android:fontFamily="@font/gotham_medium"
                            android:lineSpacingExtra="@dimen/_5sdp"
                            android:text="@string/schedule_events_set_reminders_assign_tasks"
                            android:textColor="@color/colorWhite"
                            android:textSize="@dimen/_10ssp"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="@+id/tvCarePoint"
                            app:layout_constraintTop_toBottomOf="@+id/tvCarePoint" />


                        <androidx.appcompat.widget.AppCompatTextView
                            android:id="@+id/tvTaskCount"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_5sdp"
                            android:fontFamily="@font/gotham_bold"
                            android:text="@string/hyphen"
                            android:layout_marginTop="@dimen/_12sdp"
                            android:textColor="@color/colorWhite"
                            android:textSize="@dimen/_20ssp"
                            app:layout_constraintBottom_toTopOf="@+id/tvTask"
                            app:layout_constraintStart_toEndOf="@+id/ivCarePoints"
                            app:layout_constraintTop_toBottomOf="@+id/tvNoEvent" />

                        <androidx.appcompat.widget.AppCompatTextView
                            android:id="@+id/tvTask"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_5sdp"
                            android:fontFamily="@font/gotham_medium"
                            android:text="@string/task_s_for_today"
                            android:textColor="@color/colorWhite"
                            android:textSize="@dimen/_10ssp"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintStart_toStartOf="parent" />
                    </androidx.constraintlayout.widget.ConstraintLayout>


                </androidx.constraintlayout.widget.ConstraintLayout>
            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:id="@+id/cvDiscussion"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_8sdp"
                android:onClick="@{listener::onClick}"
                android:visibility="gone"
                app:cardCornerRadius="@dimen/_18ssp"
                app:layout_constraintEnd_toEndOf="@+id/cvCarePoints"
                app:layout_constraintStart_toStartOf="@+id/cvCarePoints"
                app:layout_constraintTop_toBottomOf="@+id/cvCarePoints">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/_5F6AB2">

                    <androidx.appcompat.widget.AppCompatImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/ic_discussions"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent" />

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:paddingHorizontal="@dimen/_15sdp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintTop_toTopOf="parent">

                        <androidx.appcompat.widget.AppCompatImageView
                            android:id="@+id/ivDiscussion"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_20sdp"
                            android:src="@drawable/ic_home_discussion"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />

                        <androidx.appcompat.widget.AppCompatTextView
                            android:id="@+id/tvDiscussions"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_5sdp"
                            android:fontFamily="@font/gotham_medium"
                            android:text="@string/discussions"
                            android:textColor="@color/colorWhite"
                            android:textSize="@dimen/_18ssp"
                            app:layout_constraintStart_toEndOf="@+id/ivDiscussion"
                            app:layout_constraintTop_toTopOf="@+id/ivDiscussion" />

                        <androidx.appcompat.widget.AppCompatTextView
                            android:id="@+id/tvDiscussionMessage"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_5sdp"
                            android:layout_marginTop="@dimen/_2sdp"
                            android:fontFamily="@font/gotham_medium"
                            android:text="@string/create_and_join_conversations_among_careteam_members"
                            android:textColor="@color/colorWhite"
                            android:textSize="@dimen/_10ssp"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toEndOf="@+id/ivDiscussion"
                            app:layout_constraintTop_toBottomOf="@+id/tvDiscussions" />

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_10ssp"
                            android:orientation="horizontal"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintStart_toStartOf="parent">

                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:gravity="center"
                                android:orientation="vertical">

                                <androidx.appcompat.widget.AppCompatTextView
                                    android:id="@+id/tvUnReadMessageCount"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="@dimen/_10sdp"
                                    android:fontFamily="@font/gotham_bold"
                                    android:text="@string/hyphen"
                                    android:textColor="@color/colorWhite"
                                    android:textSize="@dimen/_20ssp" />

                                <androidx.appcompat.widget.AppCompatTextView
                                    android:id="@+id/tvUnReadMessage"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:fontFamily="@font/gotham_medium"
                                    android:text="@string/discussion_s"
                                    android:textColor="@color/colorWhite"
                                    android:textSize="@dimen/_10ssp" />
                            </LinearLayout>

                            <View
                                android:layout_width="1dp"
                                android:layout_height="match_parent"
                                android:background="@color/colorWhite" />

                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:gravity="center"
                                android:orientation="vertical">

                                <androidx.appcompat.widget.AppCompatTextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="@dimen/_10sdp"
                                    android:fontFamily="@font/gotham_bold"
                                    android:text="@string/hyphen"
                                    android:textColor="@color/colorWhite"
                                    android:textSize="@dimen/_20ssp" />

                                <androidx.appcompat.widget.AppCompatTextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:fontFamily="@font/gotham_medium"
                                    android:text="@string/unread_message_s"
                                    android:textColor="@color/colorWhite"
                                    android:textSize="@dimen/_10ssp" />
                            </LinearLayout>
                        </LinearLayout>


                    </androidx.constraintlayout.widget.ConstraintLayout>


                </androidx.constraintlayout.widget.ConstraintLayout>
            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:id="@+id/cvMedList"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_8sdp"
                android:layout_marginHorizontal="@dimen/_10sdp"
                android:visibility="gone"
                app:cardBackgroundColor="@color/_187AB6"
                android:onClick="@{listener::onClick}"
                app:cardCornerRadius="@dimen/_18ssp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/cvDiscussion">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:padding="@dimen/_15sdp"
                    android:background="@drawable/ic_med_list">



                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintTop_toTopOf="parent">

                        <androidx.appcompat.widget.AppCompatImageView
                            android:id="@+id/ivMedList"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@drawable/ic_home_med_list"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />

                        <androidx.appcompat.widget.AppCompatTextView
                            android:id="@+id/tvMedList"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_5sdp"
                            android:fontFamily="@font/gotham_medium"
                            android:text="@string/medlist"
                            android:textColor="@color/colorWhite"
                            android:textSize="@dimen/_18ssp"
                            app:layout_constraintStart_toEndOf="@+id/ivMedList"
                            app:layout_constraintTop_toTopOf="@id/ivMedList" />

                        <androidx.appcompat.widget.AppCompatTextView
                            android:id="@+id/tvMedListDescription"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_2sdp"
                            android:fontFamily="@font/gotham_medium"
                            android:text="@string/create_and_maintain_a_list_of_loved_one_medication"
                            android:textColor="@color/colorWhite"
                            android:lineSpacingExtra="@dimen/_5sdp"
                            android:textSize="@dimen/_10ssp"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="@+id/tvMedList"
                            app:layout_constraintTop_toBottomOf="@+id/tvMedList" />

                        <androidx.appcompat.widget.AppCompatTextView
                            android:id="@+id/tvMedListMessageCount"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_12sdp"
                            android:fontFamily="@font/gotham_bold"
                            android:text="0"
                            android:textColor="@color/colorWhite"
                            android:textSize="@dimen/_20ssp"
                            app:layout_constraintStart_toEndOf="@+id/ivMedList"
                            app:layout_constraintTop_toBottomOf="@+id/tvMedListDescription"
                            tools:ignore="HardcodedText" />

                        <androidx.appcompat.widget.AppCompatTextView
                            android:id="@+id/tvMedListMessage"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/gotham_medium"
                            android:text="@string/medications_for_today"
                            android:textColor="@color/colorWhite"
                            android:layout_marginTop="@dimen/_5sdp"
                            android:textSize="@dimen/_10ssp"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/tvMedListMessageCount" />

                    </androidx.constraintlayout.widget.ConstraintLayout>


                </androidx.constraintlayout.widget.ConstraintLayout>
            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:id="@+id/cvResources"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_8sdp"
                android:onClick="@{listener::onClick}"
                android:layout_marginHorizontal="@dimen/_10sdp"
                android:visibility="gone"
                app:cardCornerRadius="@dimen/_18ssp"
                app:cardBackgroundColor="@color/_0086AC"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/cvMedList">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/ic_home_resources">

<!--
                    <androidx.appcompat.widget.AppCompatImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/ic_home_resources"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent" />
-->

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:padding="@dimen/_15sdp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintTop_toTopOf="parent">

                        <androidx.appcompat.widget.AppCompatImageView
                            android:id="@+id/ivResource"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@drawable/ic_resourc"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />

                        <androidx.appcompat.widget.AppCompatTextView
                            android:id="@+id/tvResource"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_5sdp"
                            android:fontFamily="@font/gotham_medium"
                            android:text="@string/resources"
                            android:textColor="@color/colorWhite"
                            android:textSize="@dimen/_18ssp"
                            app:layout_constraintStart_toEndOf="@+id/ivResource"
                            app:layout_constraintTop_toTopOf="@id/ivResource" />

                        <androidx.appcompat.widget.AppCompatTextView
                            android:id="@+id/tvResourceListDescription"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_2sdp"
                            android:fontFamily="@font/gotham_medium"
                            android:text="@string/discover_health_and_well_being_resources_for_caregivers_and_loved_ones"
                            android:textColor="@color/colorWhite"
                            android:lineSpacingExtra="@dimen/_5sdp"
                            android:textSize="@dimen/_10ssp"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="@+id/tvResource"
                            app:layout_constraintTop_toBottomOf="@+id/tvResource" />

                        <androidx.appcompat.widget.AppCompatTextView
                            android:id="@+id/tvResourceListMessageCount"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_12sdp"
                            android:fontFamily="@font/gotham_bold"
                            android:text="@string/hyphen"
                            android:textColor="@color/colorWhite"
                            android:textSize="@dimen/_20ssp"
                            android:visibility="invisible"
                            app:layout_constraintTop_toBottomOf="@+id/tvResourceListDescription"
                            android:layout_marginStart="@dimen/_25sdp"
                            app:layout_constraintBottom_toTopOf="@+id/tvResourceListMessage"
                            app:layout_constraintStart_toStartOf="parent" />

                        <androidx.appcompat.widget.AppCompatTextView
                            android:id="@+id/tvResourceListMessage"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_5sdp"
                            android:fontFamily="@font/gotham_medium"
                            android:text="@string/new_article_and_topics"
                            android:textColor="@color/colorWhite"
                            android:textSize="@dimen/_10ssp"
                            android:visibility="invisible"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintStart_toStartOf="parent" />

                    </androidx.constraintlayout.widget.ConstraintLayout>


                </androidx.constraintlayout.widget.ConstraintLayout>
            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:id="@+id/cvLockBox"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_8sdp"
                android:onClick="@{listener::onClick}"
                android:layout_marginHorizontal="@dimen/_10sdp"
                android:visibility="gone"
                app:cardBackgroundColor="@color/_008D98"
                app:cardCornerRadius="@dimen/_18ssp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/cvResources">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/ic_home_lockbox_bg">

<!--
                    <androidx.appcompat.widget.AppCompatImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/ic_home_lockbox_bg"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent" />
-->

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:padding="@dimen/_15sdp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintTop_toTopOf="parent">

                        <androidx.appcompat.widget.AppCompatImageView
                            android:id="@+id/ivLockBox"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@drawable/ic_home_lockbox"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />

                        <androidx.appcompat.widget.AppCompatTextView
                            android:id="@+id/tvLockBox"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_5sdp"
                            android:fontFamily="@font/gotham_medium"
                            android:text="@string/lock_box"
                            android:textColor="@color/colorWhite"
                            android:textSize="@dimen/_18ssp"
                            app:layout_constraintStart_toEndOf="@+id/ivLockBox"
                            app:layout_constraintTop_toTopOf="@id/ivLockBox" />

                        <androidx.appcompat.widget.AppCompatTextView
                            android:id="@+id/tvLockBoxDescription"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_2sdp"
                            android:fontFamily="@font/gotham_medium"
                            android:text="@string/lock_box_desc"
                            android:textColor="@color/colorWhite"
                            android:lineSpacingExtra="@dimen/_5sdp"
                            android:textSize="@dimen/_10ssp"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="@+id/tvLockBox"
                            app:layout_constraintTop_toBottomOf="@+id/tvLockBox" />

                        <androidx.appcompat.widget.AppCompatTextView
                            android:id="@+id/tvLockBoxCount"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_12sdp"
                            android:fontFamily="@font/gotham_bold"
                            android:text="@string/hyphen"
                            android:textColor="@color/colorWhite"
                            android:textSize="@dimen/_20ssp"
                            app:layout_constraintStart_toEndOf="@+id/ivLockBox"
                            app:layout_constraintTop_toBottomOf="@+id/tvLockBoxDescription" />

                        <androidx.appcompat.widget.AppCompatTextView
                            android:id="@+id/tvLockBoxMessage"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_5sdp"
                            android:fontFamily="@font/gotham_medium"
                            android:text="@string/files_in_your_lockbox"
                            android:textColor="@color/colorWhite"
                            android:textSize="@dimen/_10ssp"
                            app:layout_constraintTop_toBottomOf="@+id/tvLockBoxCount"
                            app:layout_constraintStart_toStartOf="@+id/ivLockBox" />

                    </androidx.constraintlayout.widget.ConstraintLayout>


                </androidx.constraintlayout.widget.ConstraintLayout>
            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:id="@+id/cvVitalStats"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_8sdp"
                android:onClick="@{listener::onClick}"
                android:layout_marginHorizontal="@dimen/_10sdp"
                android:visibility="gone"
                app:cardCornerRadius="@dimen/_18ssp"
                app:cardBackgroundColor="@color/_41A1DA"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/cvLockBox">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/ic_vital_stats">


                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:paddingHorizontal="@dimen/_15sdp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintTop_toTopOf="parent">

                        <androidx.appcompat.widget.AppCompatImageView
                            android:id="@+id/ivVital"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_20sdp"
                            android:src="@drawable/ic_home_vital"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />

                        <androidx.appcompat.widget.AppCompatTextView
                            android:id="@+id/tvVitalStats"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_5sdp"
                            android:fontFamily="@font/gotham_medium"
                            android:text="@string/vital_stats"
                            android:textColor="@color/colorWhite"
                            android:textSize="@dimen/_18ssp"
                            app:layout_constraintStart_toEndOf="@+id/ivVital"
                            app:layout_constraintTop_toTopOf="@id/ivVital" />

                        <androidx.appcompat.widget.AppCompatTextView
                            android:id="@+id/tvVitalStatsDescription"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_2sdp"
                            android:fontFamily="@font/gotham_medium"
                            android:text="@string/remotely_monitor_your_loved_one_s_vitals_statistics_via_wearables"
                            android:textColor="@color/colorWhite"
                            android:lineSpacingExtra="@dimen/_5sdp"
                            android:textSize="@dimen/_10ssp"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="@+id/tvVitalStats"
                            app:layout_constraintTop_toBottomOf="@+id/tvVitalStats" />

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginVertical="@dimen/_12sdp"
                            android:orientation="horizontal"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/tvVitalStatsDescription">

                            <androidx.appcompat.widget.AppCompatImageView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:src="@drawable/ic_heart"
                                android:tint="@color/colorWhite" />

                            <androidx.appcompat.widget.AppCompatImageView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginHorizontal="@dimen/_10sdp"
                                android:src="@drawable/ic_temperature"
                                android:tint="@color/colorWhite" />

                            <androidx.appcompat.widget.AppCompatImageView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:src="@drawable/ic_blood_pressure"
                                android:tint="@color/colorWhite" />

                            <androidx.appcompat.widget.AppCompatImageView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginHorizontal="@dimen/_10sdp"
                                android:src="@drawable/ic_oxygen"
                                android:tint="@color/colorWhite" />
                        </LinearLayout>

                    </androidx.constraintlayout.widget.ConstraintLayout>


                </androidx.constraintlayout.widget.ConstraintLayout>
            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:id="@+id/cvCareTeam"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_8sdp"
                android:layout_marginBottom="@dimen/_5sdp"
                android:onClick="@{listener::onClick}"
                android:layout_marginHorizontal="@dimen/_10sdp"
                android:visibility="gone"
                app:cardBackgroundColor="@color/_71C8B7"
                app:cardCornerRadius="@dimen/_18ssp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/cvVitalStats">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:padding="@dimen/_15sdp"
                    android:background="@drawable/ic_care_teams">



                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintTop_toTopOf="parent">

                        <androidx.appcompat.widget.AppCompatImageView
                            android:id="@+id/ivCareTeam"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@drawable/ic_home_care_team"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />

                        <androidx.appcompat.widget.AppCompatTextView
                            android:id="@+id/tvCareTeam"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_5sdp"
                            android:fontFamily="@font/gotham_medium"
                            android:text="@string/care_team"
                            android:textColor="@color/colorWhite"
                            android:textSize="@dimen/_18ssp"
                            app:layout_constraintStart_toEndOf="@+id/ivCareTeam"
                            app:layout_constraintTop_toTopOf="@+id/ivCareTeam" />

                        <androidx.appcompat.widget.AppCompatTextView
                            android:id="@+id/tvMember"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_2sdp"
                            android:fontFamily="@font/gotham_medium"
                            android:text="@string/create_care_team"
                            android:textColor="@color/colorWhite"
                            android:lineSpacingExtra="@dimen/_5sdp"
                            android:textSize="@dimen/_10ssp"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="@+id/tvCareTeam"
                            app:layout_constraintTop_toBottomOf="@+id/tvCareTeam" />

                        <androidx.recyclerview.widget.RecyclerView
                            android:id="@+id/rvCareTeam"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginVertical="@dimen/_12sdp"
                            android:orientation="horizontal"
                            app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/tvMember"
                            tools:itemCount="5"
                            tools:listitem="@layout/adapter_care_team_members_dashboard" />



                    </androidx.constraintlayout.widget.ConstraintLayout>


                </androidx.constraintlayout.widget.ConstraintLayout>
            </androidx.cardview.widget.CardView>
        </androidx.constraintlayout.widget.ConstraintLayout>


    </androidx.core.widget.NestedScrollView>
</layout>